#include <stdio.h>
#include <string.h>

const int MAX_SIZE = 5;

int main() {
  char *history[MAX_SIZE];
  char *input = NULL;
  size_t len = 0;
  int arr_size = 0;

  while (1) {
    printf("Enter input: ");
    len = getline(&history[arr_size], &len, stdin);
    history[arr_size][len - 1] = '\0';
    arr_size++;

    if (strcmp(input, "print") == 0) {
      for (int i = 0; i < arr_size; i++) {
        printf("%s\n", history[i]);
      }
    }
  }

  return 0;
}
